{
  "framework": "vite",
  "buildCommand": "npm install && npm run build",
  "outputDirectory": "dist",
  "installCommand": "npm install",
  "devCommand": "npm run dev",
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "^/(?!_next|assets|favicon)(.+\\.(html|htm|pdf|txt|md|json|xml|css|js|png|jpg|jpeg|gif|svg|webp|ico|mp3|mp4|wav|webm|woff|woff2|ttf|eot|zip|tar|gz))$",
      "dest": "/api/serve/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
